<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PressTextView">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="fontFamily" format="string" />
        <attr name="gravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
        <attr name="textAlignment" format="integer">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
            <enum name="center" value="2" />
            <enum name="gravity" value="3" />
        </attr>
        <attr name="textDirection" format="integer">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
        </attr>
        <attr name="lineSpacing" format="dimension" />
        <attr name="lineSpacingMultiplier" format="float" />
        <attr name="includeFontPadding" format="boolean" />
        <attr name="ellipsize" format="integer">
            <enum name="start" value="0" />
            <enum name="middle" value="1" />
            <enum name="end" value="2" />
            <enum name="marquee" value="3" />
        </attr>
        <attr name="maxLines" format="integer" min="1" />
    </declare-styleable>
</resources>